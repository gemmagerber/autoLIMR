citHeader("To cite autoLIMR in publications use:")

citEntry(
  entry    = "Unpublished",
  note     = "in review",
  title    = "'autoLIMR': including data uncertainty in
    ecological network models with linear inverse modelling and Markov Chain
    Monte Carlo",
  author   = "Gemma Gerber",
  year     = "2023",
  url      = "https://github.com/gemmagerber/autoLIMR",
  textVersion = "Gerber, G. 2023. 'autoLIMR': including data uncertainty in
    ecological network models with linear inverse modelling and Markov Chain
    Monte Carlo. R package v 2.2.0. URL: https://github.com/gemmagerber/autoLIMR"
)
